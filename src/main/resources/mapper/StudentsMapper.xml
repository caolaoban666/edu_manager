<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ty.hh.edu.mapper.StudentsMapper">

    <select id="selectByPhone" resultType="com.ty.hh.edu.model.entity.StudentsEntity">
        SELECT * FROM students WHERE phone = #{phone}
    </select>
    <select id="listByUserId" resultType="com.ty.hh.edu.model.entity.StudentsEntity">
        SELECT * FROM students WHERE user_id = #{userId}
    </select>
</mapper>
